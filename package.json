{
  "name": "mutent",
  "version": "5.0.0-9",
  "description": "Elegant and agnostic solution to work with any persistence layer",
  "main": "./mutent.js",
  "types": "./mutent.d.ts",
  "files": [
    "mutent.js",
    "mutent.d.ts"
  ],
  "keywords": [
    "agnostic",
    "database",
    "entity",
    "immutable",
    "iterable",
    "mutator",
    "mutation",
    "store",
    "stream"
  ],
  "scripts": {
    "lint": "standard --fix src",
    "test": "nyc --reporter=lcov --reporter=text-summary ava",
    "build": "rollup -c",
    "prepublishOnly": "npm run build && npm run test"
  },
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "devDependencies": {
    "ava": "^3.15.0",
    "esm": "^3.2.25",
    "nyc": "^15.1.0",
    "rollup": "^2.58.0",
    "standard": "^16.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent.git"
  },
  "ava": {
    "extensions": [
      "js"
    ],
    "files": [
      "**/*.spec.js"
    ],
    "require": [
      "esm"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
