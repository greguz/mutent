{
  "name": "mutent",
  "version": "5.0.0-10",
  "description": "Elegant and agnostic solution to work with any persistence layer.",
  "type": "module",
  "exports": {
    "import": "./mutent.mjs",
    "require": "./mutent.cjs"
  },
  "main": "./mutent.cjs",
  "types": "./mutent.d.mts",
  "files": [
    "!**/*.spec.mjs",
    "lib",
    "mutent.mjs",
    "mutent.cjs",
    "mutent.d.mts"
  ],
  "keywords": [
    "agnostic",
    "database",
    "entity",
    "immutable",
    "iterable",
    "mutator",
    "mutation",
    "store",
    "stream"
  ],
  "scripts": {
    "lint": "standard",
    "test": "c8 ava",
    "build": "rollup -c",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "devDependencies": {
    "ava": "^4.3.0",
    "c8": "^7.11.3",
    "rollup": "^2.75.6",
    "standard": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent.git"
  }
}
